# Vercel Postgres Database Connection
# Get these values by running: vercel env pull .env.local
# Or set them manually if using a different Postgres instance
POSTGRES_URLSTATE=postgresql://user:password@host:port/database

# API Configuration
# For development with vercel dev, use localhost:3000/api
# For production, this will use the deployed Vercel URL
VITE_API_URL=http://localhost:3000/api

# Environment
NODE_ENV=development
